#include <stdio.h>
int main()
{
int a[100],key,n=7;
int i,flag=0;
for(i=0;i<n;i++){
printf("Enter the array elemnt :");
scanf("%d",&a[i]);
}
printf(Enter key :");
scanf("%d",&key);
for(i=0;i<n;i++)
{
if(a[i]==key)
{
printf("element is found at %d",i);
flag=1;
break;
}}
if(flag=0)
printf("Error");
}
